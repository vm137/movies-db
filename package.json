{
  "name": "movies-db",
  "version": "1.0.0",
  "description": "react movie database",
  "scripts": {
    "start": "cross-env NODE_ENV=development  node ./server",
    "start-prod": "npm run build && cross-env NODE_ENV=production node ./server",
    "build": "webpack --mode=production --config ./webpack",
    "lint": "eslint --ext js,jsx src server webpack",
    "test": "jest src",
    "test:cover": "jest src --coverage",
    "test:watch": "jest src --watch",
    "test:update-snapshots": "npm test -- -u",
    "flow": "flow",
    "ci": "npm run lint && npm run flow && npm test && npm run build",
    "cypress": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vm137/movies-db.git"
  },
  "keywords": [
    "movie",
    "database"
  ],
  "author": "Victor Mikhaylov",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/vm137/movies-db/issues"
  },
  "homepage": "https://github.com/vm137/movies-db#readme",
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-hot-loader": "^4.8.4",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.3",
    "webpack": "^4.29.6",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.24.4",
    "webpack-hot-server-middleware": "^0.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-preset-flow": "^6.23.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^2.1.1",
    "cypress": "^3.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.98.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "mini-css-extract-plugin": "^0.4.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "verbose": true
  }
}
